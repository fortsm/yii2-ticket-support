{
    "name": "fortsm/yii2-ticket-support",
    "description": "Yii2 Support Ticket Module, easy, flexible, fast",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "support",
        "ticket",
        "module"
    ],
    "license": "MIT",
    "authors": [{
        "name": "fortsm",
        "email": "akiraz@bk.ru"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [{
        "type": "composer",
        "url": "https://asset-packagist.org"
    }],
    "require": {
        "yiisoft/yii2": "2.0.32",
        "hashids/hashids": "^2.0",
        "yiidoc/yii2-redactor": "*",
        "php-imap/php-imap": "^3.0",
        "yiisoft/yii2-queue": "~2.3",
        "rmrevin/yii2-fontawesome": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "fortsm\\support\\": ""
        }
    },
    "extra": {
        "bootstrap": "fortsm\\support\\Bootstrap"
    }
}